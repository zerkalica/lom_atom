{
  "name": "lom_atom",
  "version": "1.0.0",
  "description": "Observable state management",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "dest": "."
  },
  "scripts": {
    "postversion": "git push && git push --tags",
    "prepush": "npm test && npm run build",
    "update": "ncu -ua && npm install",
    "clean": "rm -rf dist",
    "build": "npm run clean && BABEL_ENV=rollup rollup -c",
    "ex": "npm run clean && BABEL_ENV=rollup rollup -c -w --config rollup.config.examples.js",
    "test": "flow check && BABEL_ENV=commonjs mocha",
    "test.dev": "BABEL_ENV=commonjs mocha --growl --watch",
    "watch": "chokidar --initial -d 2000 -t 1000 'src/**/*.js' -c 'npm run build && cp -rvf *.js src dist $npm_package_config_dest/node_modules/lom_atom'"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/lom_atom.git"
  },
  "keywords": [
    "state",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "*.flow",
    ".flowconfig",
    "README.md",
    "LICENSE",
    "src",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "flow-bin": "^0.50.0",
    "husky": "^0.14.3",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.3",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-serve": "^0.4.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "sinon": "^2.3.8"
  },
  "dependencies": {
    "fetch-mock": "^5.12.1",
    "jss": "^8.1.0",
    "jss-camel-case": "^5.0.0",
    "todomvc-app-css": "^2.1.0",
    "todomvc-common": "^1.0.3",
    "uuid": "^3.1.0"
  }
}
